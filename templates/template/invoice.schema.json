    "required": [
        "custom",
        "sample"
    ],
    "properties": {
        "custom": {
            "type": "object",
            "label": {
                "ja": "固有情報",
                "en": "Custom Information"
            },
            "required": [],
            "properties": {
                "measurement_data_start_character": {
                    "label": {
                        "ja": "計測データ開始文字列",
                        "en": "Measurement Data Start Character"
                    },
                    "type": "string",
                    "description": "計測データ開始行となる文字列を記載",
                    "options": {
                        "placeholder": {
                            "ja": "計測データ開始キーワードを入力",
                            "en": "Enter the keyword that marks the start of measurement data"
                        }
                    }
                },
                "x-axis_column_index": {
                    "label": {
                        "ja": "x軸列番号",
                        "en": "x-axis column index"
                    },
                    "type": "number",
                    "options": {
                        "placeholder": {
                            "ja": "x軸プロットデータの列番号(1始まり)を入力してください。",
                            "en": "Please enter the column number for the x-axis plot data (starting from 1)"
                        }
                    },
                    "minimum": 1.0
                },
                "y-axis_column_index": {
                    "label": {
                        "ja": "y軸列番号",
                        "en": "y-axis column index"
                    },
                    "type": "number",
